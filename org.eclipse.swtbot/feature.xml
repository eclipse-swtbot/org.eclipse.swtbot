<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.swtbot"
      label="SWTBot for SWT Testing"
      version="4.3.0.qualifier"
      provider-name="Eclipse.org - SWTBot"
      plugin="org.eclipse.swtbot.eclipse.ui"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="http://www.eclipse.org/swtbot/">
      SWTBot for testing SWT-based applications
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="SWTBot update site" url="http://download.eclipse.org/technology/swtbot/releases/latest/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.junit"/>
      <import plugin="org.hamcrest.core"/>
      <import plugin="org.hamcrest.library"/>
   </requires>

   <plugin
         id="org.eclipse.swtbot.swt.finder"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.swtbot.junit4_x"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
